list1 = []
for i in range(0, 3):
    list1.append(int(input(f'请输入第{i+1}个数字:')))

for i in range(0, 2):
    if list1[i] > list1[i+1]:
        tmp = list1[i]
        list1[i] = list1[i+1]
        list1[i+1] =tmp

if list1[0] > list1[1]:
    tmp = list1[0]
    list1[0] = list1[1]
    list1[1] = tmp
print(list1)
