class bcolors:
    HEADER = '\033[95m' #紫色
    OKBLUE = '\033[94m' #蓝色
    OKGREEN = '\033[92m' #绿色
    WARNING = '\033[93m' #橘黄
    FAIL = '\033[91m' #红色
    ENDC = '\033[0m' #字体
    BOLD = '\033[1m' #字体
    UNDERLINE = '\033[4m' #下划线

print(bcolors.FAIL+ "警告的颜色字体?" + bcolors.ENDC )
print(bcolors.OKBLUE + "警告的颜色字体?" + bcolors.BOLD)
