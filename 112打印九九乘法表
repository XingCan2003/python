# 打印九九乘法表
# for循环
for i in range(1,10):
    for j in range(1,i):
        print(f'{i} * {j} = %2d ' % (i*j),end="")
    print()

#while循环
i=0
j=0
while i<9:
    i+=1
    while j<9:
        j+=1
        print(j,"x",i,"=",i*j,"\t",end="")
        if i==j:
            j=0
            print("")
            break
